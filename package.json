{
  "private": true,
  "scripts": {
    "exe": "ts-node --transpile-only",
    "fix": "npm-run-all --continue-on-error \"fix:*\"",
    "fix:eslint": "eslint --fix \"**/*\"",
    "fix:markdownlint": "markdownlint --fix \"**/*\"",
    "fix:prettier": "prettier --write \"**/*\"",
    "fix:yarn-deduplicate": "yarn install; yarn-deduplicate --strategy=fewer; yarn install",
    "lint": "npm-run-all --continue-on-error \"lint:*\"",
    "lint:eslint": "eslint \"**/*\" --report-unused-disable-directives",
    "lint:markdownlint": "markdownlint \"**/*\"",
    "lint:prettier": "prettier --check \"**/*\"",
    "lint:tsc": "tsc --project .",
    "lint:yarn-deduplicate": "yarn-deduplicate --fail --list --strategy=fewer",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "run-if-changed",
      "post-checkout": "run-if-changed",
      "post-merge": "run-if-changed",
      "post-rewrite": "run-if-changed"
    }
  },
  "lint-staged": {
    "**/*": [
      "eslint --fix",
      "markdownlint --fix",
      "prettier --write"
    ]
  },
  "resolutions": {
    "@mapbox/geojson-rewind": "^0.5.0",
    "xmldom": "^0.5.0"
  },
  "devDependencies": {
    "@hkdobrev/run-if-changed": "^0.3.1",
    "@kachkaev/commands": "^0.1.1",
    "@kachkaev/eslint-config-base": "^0.2.3",
    "@kachkaev/markdownlint-config": "^0.2.0",
    "@kachkaev/prettier-config": "^1.2.0",
    "@mapbox/tilebelt": "^1.0.2",
    "@tmcw/togeojson": "^4.3.0",
    "@turf/turf": "^6.3.0",
    "@types/command-exists": "^1.2.0",
    "@types/dedent": "^0.7.0",
    "@types/fs-extra": "^9.0.8",
    "@types/jest": "^26.0.21",
    "@types/js-yaml": "^4.0.0",
    "@types/lodash": "^4.14.168",
    "@types/lru-cache": "^5.1.0",
    "@types/luxon": "^1.26.2",
    "@types/mapbox__tilebelt": "^1.0.0",
    "@types/node": "^14.14.35",
    "@types/nprogress": "^0.2.0",
    "@types/object-hash": "^1.3.4",
    "@types/sharp": "^0.27.1",
    "@types/xmldom": "^0.1.30",
    "axios": "^0.21.1",
    "axios-retry": "^3.1.9",
    "canvas": "^2.7.0",
    "chalk": "^4.1.0",
    "command-exists": "^1.2.9",
    "dedent": "^0.7.0",
    "dotenv": "^8.2.0",
    "envalid": "^7.1.0",
    "eslint": "^7.22.0",
    "eslint-plugin-unicorn": "^29.0.0",
    "execa": "^5.0.0",
    "fs-extra": "^9.1.0",
    "globby": "^11.0.3",
    "husky": "^4.3.8",
    "immer": "^9.0.1",
    "jest": "^26.6.3",
    "js-yaml": "^4.0.0",
    "lint-staged": "^10.5.4",
    "lodash": "^4.17.21",
    "luxon": "^1.26.0",
    "markdownlint-cli": "^0.27.1",
    "npm-run-all": "^4.1.5",
    "osmtogeojson": "^3.0.0-beta.4",
    "p-queue": "^6.6.2",
    "prettier": "^2.2.1",
    "rm-up": "^1.0.4",
    "sharp": "^0.27.2",
    "sleep-promise": "^9.1.0",
    "sort-keys": "^4.2.0",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "vega": "^5.20.0",
    "vega-lite": "^5.0.0",
    "xmldom": "^0.5.0",
    "yarn-deduplicate": "^3.1.0"
  },
  "engines": {
    "node": ">=14.15.1",
    "yarn": ">=1.22.0"
  },
  "run-if-changed": {
    "yarn.lock": "yarn install --prefer-offline --pure-lockfile"
  }
}
